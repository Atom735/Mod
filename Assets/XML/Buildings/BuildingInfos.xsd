<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="x-schema:CIV4BuildingsSchema.xml" xmlns:c="x-schema:CIV4BuildingsSchema.xml">
    <xs:element name="Civ4BuildingInfos">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="c:BuildingInfos"/>
            </xs:sequence>
